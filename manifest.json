{
  "manifest_version": 2,
  "name": "jsTask",
  "version": "1.0",
  "description": "jsTask",
  "browser_action": {
    "default_title": "jsTask",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [ "<all_urls>" ],
      "js": ["elemTextTranslation.js","mutation.js","revertTranslation.js"]
    }
  ],
  "permissions": [
    "tabs"
  ]
}